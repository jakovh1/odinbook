<% content_for :title, "Posts" %>

<h1>Posts</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <%= render partial: "post", locals: { post: post, user: current_user } %>
  <% end %>
</div>
<%= link_to "New post", new_post_path %>

<% if false %>
  <div id="incoming-requests-sidebar">
    <%= render "incoming_pending_requests", incoming_follow_requests: @incoming_follow_requests %>
  </div>
<% end %>